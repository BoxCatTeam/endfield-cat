{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "endfield-cat",
  "version": "0.1.2",
  "identifier": "org.boxcat.endfield-cat",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "endfield-cat",
        "width": 1175,
        "height": 875,
        "minWidth": 675,
        "minHeight": 475,
        "decorations": false,
        "center": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      }
    }
  },
  "plugins": {
    "sql": {
      "preload": []
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
